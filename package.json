{
  "private": true,
  "name": "calendar-moji",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "build": "preact build --no-prerender",
    "serve": "preact build && preact serve",
    "dev": "preact watch",
    "lint": "eslint src",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watchAll",
    "deploy:stg": "NODE_ENV=staging npm run build && firebase deploy -P default",
    "deploy:prd": "NODE_ENV=production npm run build && firebase deploy -P production"
  },
  "eslintIgnore": [
    "build/*"
  ],
  "devDependencies": {
    "classnames": "^2.2.5",
    "eslint": "^4.5.0",
    "eslint-config-synacor": "^1.1.0",
    "if-env": "^1.0.0",
    "jest": "^22.0.4",
    "node-sass": "^4.7.2",
    "preact-cli": "^2.1.1",
    "sass-loader": "^6.0.6",
    "worker-loader": "^1.1.0"
  },
  "dependencies": {
    "date-fns": "^1.29.0",
    "dlv": "^1.1.0",
    "firebase": "^4.6.2",
    "googleapis": "^23.0.0",
    "idb": "^2.0.4",
    "mitt": "^1.1.3",
    "preact": "^8.2.1",
    "preact-compat": "^3.17.0",
    "preact-router": "^2.5.5",
    "siema": "^1.4.9",
    "unfetch": "^3.0.0",
    "unistore": "^3.0.4"
  }
}
